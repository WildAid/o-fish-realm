{
    "database": "wildaid",
    "collection": "Agency",
    "roles": [
        {
            "name": "Global Admin",
            "apply_when": {
                "%%true": {
                    "%function": {
                        "name": "isGlobalAdmin",
                        "arguments": [
                            "%%user.data.email"
                        ]
                    }
                }
            },
            "write": true,
            "insert": true,
            "delete": true,
            "additional_fields": {}
        }
    ],
    "schema": {}
}
