{
    "database": "wildaid",
    "collection": "Photo",
    "roles": [
        {
            "name": "Global Admin",
            "apply_when": {
                "%%true": {
                    "%function": {
                        "name": "isGlobalAdmin",
                        "arguments": [
                            "%%user.data.email"
                        ]
                    }
                }
            },
            "write": true,
            "insert": true,
            "delete": true,
            "additional_fields": {}
        },
        {
            "name": "Agency Admin",
            "apply_when": {
                "%%true": {
                    "%function": {
                        "name": "isAgencyAdmin",
                        "arguments": [
                            "%%root.agency",
                            "%%user.data.email"
                        ]
                    }
                }
            },
            "write": true,
            "insert": true,
            "delete": true,
            "additional_fields": {}
        },
        {
            "name": "Agency Member",
            "apply_when": {
                "%%true": {
                    "%function": {
                        "name": "isAgencyMember",
                        "arguments": [
                            "%%root.agency",
                            "%%user.data.email"
                        ]
                    }
                }
            },
            "read": true,
            "insert": true,
            "delete": true,
            "additional_fields": {}
        }
    ],
    "schema": {}
}
